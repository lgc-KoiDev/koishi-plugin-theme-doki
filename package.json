    "@koishijs/plugin-console": "^5.17.0",
    "@vueuse/core": "^10.9.0",
    "cosmokit": "^1.5.2",
    "koishi-plugin-wallpaper": "^1.0.4",
    "octokit": "^3.1.2",
    "semaphore-promise": "^1.4.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vue": "^3.4.21"
